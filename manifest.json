{
"manifest_version": 2,
    "name": "Davidinator",
    "description": "Change names & words to EXTRAORDINARY names & words for your viewing pleasure.",
    "version": "3",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}